# Token du bot Discord (obligatoire)
DISCORD_TOKEN=your_bot_token_here

# ID de l'application Discord (obligatoire)
DISCORD_CLIENT_ID=your_client_id_here

# ID du serveur pour le développement (optionnel)
# Si défini, les commandes seront enregistrées uniquement sur ce serveur
# Laisser vide pour enregistrer les commandes globalement
DISCORD_GUILD_ID=

